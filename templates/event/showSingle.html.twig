    decoder : {
      readers : ["code_128_reader", "ean_reader", "ean_8_reader", "code_39_reader", "code_39_vin_reader", "codabar_reader", "upc_reader", "upc_e_reader", "i2of5_reader", "2of5_reader", "code_93_reader"]
    }
  }, function(err) {
    if (err) {
      console.log(err);
      return
    }
    console.log("QuaggaJS successfully initialized.");
   
  });
  Quagga.start();

 
});
 document.querySelector('#btnClose').addEventListener('click', function() {
    // Stop the camera stream
    Quagga.stop();
  });
  // Attach listeners to the QuaggaJS events to handle the captured images.
  Quagga.onDetected(function(data) {
    console.log("QR code detected: " + data.codeResult.code);
    // Submit the form when a QR code is detected.
    document.querySelector('#hidden-form').submit();
  });
 
</script>
