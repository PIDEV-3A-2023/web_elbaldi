<form method="post" action="{{ path('app_categorie_delete', {'id_categorie': categorie.id_categorie}) }}" onsubmit="return confirm('Êtes-vous sûr(e) de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.id_categorie) }}">
    <button class="btn">Supprimer</button>
</form>
